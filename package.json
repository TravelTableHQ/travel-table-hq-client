{
  "name": "travel-table-hq-workspace",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "pnpm exec eslint --fix",
    "**/*.{md,json,css,scss}": "pnpm exec prettier --write"
  }
}
